{
  "manifest_version": 3,
  "name": "Tab Editor",
  "version": "1.0",
  "description": "Rename your tab to find specific tab easily, especially among tabs with same title.",
  "permissions": [
    "contextMenus",
    "scripting",
    "tabs",
    "storage"
  ],
  "host_permissions":[
    "http://*/*",
    "https://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "default_locale": "en"
}
